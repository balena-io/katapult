{
  "name": "katapult",
  "description": "A tool for launching container-based products",
  "version": "1.6.0",
  "private": true,
  "author": "Michael Angelos Simos <michalis@balena.io>",
  "bin": {
    "katapult": "./bin/run"
  },
  "bugs": "https://github.com/balena-io/katapult/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.10",
    "@oclif/plugin-help": "^2.1.6",
    "@types/bluebird": "^3.5.26",
    "@types/inquirer": "^6.0.1",
    "@types/validator": "^10.11.0",
    "@types/yamljs": "^0.2.30",
    "asn1js": "^2.0.22",
    "base32-encode": "^1.1.1",
    "bluebird": "^3.5.3",
    "dhparam": "^2.0.0",
    "dotenv": "^7.0.0",
    "inquirer": "^6.3.1",
    "jsonschema": "^1.2.4",
    "kubernetes-client": "^6.11.0",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "mv": "^2.1.1",
    "node-forge": "^0.8.1",
    "pkijs": "^2.1.77",
    "pvutils": "^1.0.17",
    "simple-git": "^1.107.0",
    "tslib": "^1.9.3",
    "tunnel-ssh": "^4.1.4",
    "validator": "^10.11.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.1",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "resin-lint": "^3.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/balena-io/katapult",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "katapult",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --config ./node_modules/resin-lint/config/.prettierrc --write",
      "resin-lint --typescript --no-prettier",
      "git add"
    ]
  },
  "repository": "balena-io/katapult",
  "scripts": {
    "build": "tsc",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"src/**/*.ts\" \"typings/**/*.ts\"",
    "lint": "resin-lint --typescript src/ && tsc --noEmit",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
